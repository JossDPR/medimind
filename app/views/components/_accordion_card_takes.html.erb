	<main>
		<ul class="item-list">
      <% elements.each do |element| %>
        <% if current_user.patient? && element.taken_date != nil %>
          <%= render "components/accordion_card_detail_takes", element: element %>
          <% elsif current_user.tutor? %>
            <%= render "components/accordion_card_detail_takes", element: element %>
        <% end %>
      <% end %>
		</ul>
	</main>
